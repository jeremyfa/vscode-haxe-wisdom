{
	"name": "haxe-wisdom",
	"version": "0.1.2",
	"publisher": "jeremyfa",
	"author": {
		"name": "Jérémy Faivre"
	},
	"displayName": "Haxe Wisdom",
	"description": "Haxe Wisdom library syntax highlighting for Visual Studio Code",
	"keywords": [
		"haxe",
		"markup",
		"vdom",
		"wisdom",
		"react",
		"highlight",
		"syntax"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jeremyfa/vscode-haxe-wisdom"
	},
	"bugs": {
		"url": "https://github.com/jeremyfa/vscode-haxe-wisdom/issues"
	},
	"homepage": "https://github.com/jeremyfa/vscode-haxe-wisdom",
	"icon": "images/haxe-logo.png",
	"galleryBanner": {
		"theme": "dark",
		"color": "#A84B38"
	},
	"engines": {
		"vscode": "^1.9.0"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"grammars": [
			{
				"path": "./syntaxes/injection.json",
				"scopeName": "source.hx.wisdom",
				"injectTo": [
					"source.hx"
				],
				"embeddedLanguages": {
					"meta.embedded.block.wisdom": "haxe"
				},
				"tokenTypes": {
					"meta.wisdom.hx": "other",
					"tag.open.hx": "other",
					"tag.closed.hx": "other"
				}
			}
		]
	},
	"dependencies": {}
}